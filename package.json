{
  "name": "randoDB",
  "version": "0.3.0",
  "description": "Module that provide API for for work with rando database",
  "engine": {
    "node": ">=0.8.8"
  },
  "author": "Rando 4Me team",
  "main": "./lib/randoDB",
  "directories": "lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/RandoApp/Rando-db.git"
  },
  "dependencies": {
    "mongodb": "2.2.x",
    "mongoose": "4.5.x",
    "winston": "2.2.0",
    "async": "2.0.0-rc.6",
    "js-yaml": "3.6.1",
    "config": "1.21.0"
  },
  "devDependencies": {
    "mocha": "2.5.3",
    "should": "11.2.1",
    "sinon": "1.17.4",
    "istanbul": "0.4.4",
    "coveralls": "2.11.9"
  },
  "scripts": {
    "test": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- --recursive",
    "coveralls": "node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --recursive && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  }
}
